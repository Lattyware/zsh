# Use nano as the default editor.
export EDITOR="/bin/nano"

# Should be reasonable for any emulator I'm going to use.
export TERM="xterm-256color"

# Use BuildKit, it's good.
export DOCKER_BUILDKIT=1
export DOCKER_CONFIG=~/.config/docker

# Prompt config.
export STARSHIP_CONFIG="${CONFIG_PATH}/starship.toml"
